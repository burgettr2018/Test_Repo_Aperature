<% digital_data(global: {pageSection: 'home'}) %>
<div id="main_content" data-section="" data-page="">
	<div class="container">
		<div class="white-wrap">
			<div class="row">
				<div class="col-sm-12">
					<div class="page-header">
						<h1><%= t('devise.shared.welcome') %> <%= current_user.name %></h1>
					</div>
				</div>
			</div>
			<div class="row">
				<div class="col-sm-12">
          <% if @links.present? %>
            <% @links.each do |l| %>
              <p><a href="<%= l.first %>"><%= l.second %></a></p>
            <% end %>
          <% end %>
				</div>
			</div>
		</div>
	</div>
</div>
